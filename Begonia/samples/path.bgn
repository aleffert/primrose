import Geometry

backend Cocoa

module Path

extern Class Path

local uiBezierPath = # Cocoa UIBezierPath : CocoaClass

make {} = {
	path = # uiBezierPath bezierPath : Path
	addLine {to : Point} = # path {addLineToPoint = to} : Unit
	move {to : Point} = # path {moveToPoint = to} : Unit
	close = # path close : Unit

}