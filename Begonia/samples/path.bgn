import Cocoa

external type Path

local nsBezierPath = Cocoa.classNamed.NSBezierPath : CocoaClass

let make {} = {
	path = nsBezierPath.bezierPath : Path
	addLine {to : Point} = path {addLineToPoint = to} : {}
	move {to : Point} = path {moveToPoint = to} : {}
	close {} = path.closePath : {}
}

let x = make {}